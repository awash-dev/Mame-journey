generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Page {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  title       String   @unique
  tags        String?
  description String?
  imgUrl      String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
